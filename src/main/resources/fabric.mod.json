{
  "schemaVersion": 1,
  "id": "autoelytra",
  "version": "${version}",
  "name": "Auto Elytra",
  "description": "A mod that automatically equips your elytra when you try to fly.",
  "authors": [
    "NimajnebEC"
  ],
  "contact": {
    "repo": "https://github.com/NimajnebEC/AutoElytra"
  },
  "license": "MIT",
  "icon": "assets/autoelytra/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.nimajnebec.autoelytra.AutoElytra"
    ]
  },
  "mixins": [
    "autoelytra.mixins.json"
  ],
  "accessWidener": "autoelytra.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}"
  }
}
